#PLEASE MODIFY THE CODE OTHERWISE THERE WILL BE CONTENT VIOLATION
class Solution:
    def answerQueries(self, nums: List[int], queries: List[int]) -> List[int]:
        self.es = []
        def printSubsequences(arr, index, subarr):
            if index == len(arr):
                if len(subarr) != 0:
                    self.es.append(subarr)

            else:
                printSubsequences(arr, index + 1, subarr)
                printSubsequences(arr, index + 1, 
                                    subarr+[arr[index]])

            return
        printSubsequences(nums, 0, [])
        result = []
        for n in queries:
            temp = 0
            for num in self.es:
                if sum(num) <= n:
                    temp = max(temp, len(num))
            result.append(temp)
        return result
